/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	
	Or(a=a, b=b, out=orOut);
	And(a=a, b=b, out=andOut);
	Not(in=andOut, out=andNotOut);
	
	And(a=orOut, b=andNotOut, out=out);
}