 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

// Rengang (Angelo) Yang
// CSCE 312 - 503
// UIN: 526009738
// Project 1
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
	Nor(a=a,b=b,out=norOut);
	Not(in=norOut, out=out);
}
